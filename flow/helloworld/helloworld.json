{
    "nodes": {
        "C001": {
            "name": "copy_flow",
            "config": {
                "path": "{base_path}/flow/helloworld.json"
            },
            "script": "copy_files.py",
            "type": "component"
        },
        "C005": {
            "name": "shell",
            "config": {
                "variables": {},
                "commands": {
                    "find_hello_world": "find . -type f -name helloworld.json",
                    "cat_flow": "cat {find_hello_world}"
                }
            },
            "script": "command.py",
            "type": "component"
        },
        "C002": {
            "name": "sleep",
            "config": {
                "seconds": 1
            },
            "script": "sleep.py",
            "type": "component"
        },
        "C003": {
            "name": "copy_helloworld",
            "config": {
                "match": [
                    ".*hello_world.*"
                ]
            },
            "script": "copy_files.py",
            "type": "component"
        }
    },
    "dependencies": {
        "C003": [
            "C005"
        ],
        "C002": [
            "C003"
        ]
    },
    "LOGGING_LEVEL": "INFO",
    "WORKERS": 20
}